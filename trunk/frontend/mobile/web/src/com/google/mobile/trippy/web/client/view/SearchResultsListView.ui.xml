<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  <ui:with field='res' type='com.google.mobile.trippy.web.client.TrippyBundle' />
  <ui:style>
    .panel {
      overflow: auto;
    }
    .noResults {
      padding: 3px;
    }
 </ui:style>
 <g:FlowPanel>
   <g:Label ui:field="noSearchResults" addStyleNames="{res.commonStyle.baseFont} {style.noResults}" visible="false"> No Search Results.</g:Label>
   <g:FlowPanel ui:field='searchItemList' addStyleNames="{style.panel}"></g:FlowPanel>
 </g:FlowPanel>
</ui:UiBinder>
